<Window x:Class="VoorbeeldOvererveringPolymorfisme.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoorbeeldOvererveringPolymorfisme"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <DockPanel>
            <Button Name="BtnStudent" Content="Student"  Width="100" Height="30" Margin="8" Click="BtnStudent_Click" />
            <Button Name="BtnLector" Content="Lector"  Width="100" Height="30" Margin="8" Click="BtnLector_Click"/>
            <Button Name="BtnPersoon" Content="Persoon"  Width="100" Height="30" Margin="8" Click="BtnPersoon_Click"/>
            <TextBox Name="TxtEmail" Margin="8" Width="200" />
        </DockPanel>
        <TextBlock Name="TxtResultaat" Height="300" Margin="10" />
    </StackPanel>
</Window>
